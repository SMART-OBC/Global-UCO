        <template id="custom_contract_layout">
            <div class="header">
                <t t-set="docs" t-value="docs.with_context(lang=lang)"/>
                <div class="row">
                    <div class="head_cont">
                    </div>
                    <div class="head_logo">
                        <img t-if="res_company.logo"
                             t-att-src="image_data_uri(res_company.logo)"
                             alt="logo"
                             style="height:50px; width:150px; position: absolute; right: 25mm; top:0mm;"/>
                        <div class="partner" style="position: absolute; right: 40mm; top:20mm; width:220px;">
                            <div>
                                <t t-esc="docs.partner_id.name"/>
                            </div>
                            <div>
                                <t t-esc="docs.partner_id.street"/>
