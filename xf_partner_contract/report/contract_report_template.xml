        <template id="contract_report_template">
            <t t-call="web.html_container">
                <t t-set="lang" t-value="en_US"/>
                <t t-call="xf_partner_contract.custom_contract_layout" t-lang="lang">
                    <div class="page" style="background-image:url('/xf_partner_contract/static/description/back.jpg');
                        background-size:235mm 250mm; padding-top:0mm; font-family: Arial, Helvetica, sans-serif; font-weight: 8; line-height:1; font-size: .9rem;">
