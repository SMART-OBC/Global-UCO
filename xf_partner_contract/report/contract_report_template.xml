        <template id="first_product">
            <t t-set="result" t-value="result.with_context(lang=lang)"/>
            <t t-set="docs" t-value="docs.with_context(lang=lang)"/>
            <div class="text-left" style="margin-left:20mm; margin-right:20mm;">
                <h2>
                    <t t-if="docs.type =='sale'">
                        Sales order
                    </t>
                    <t t-else="">
                        Purchase order
                    </t>
                </h2>
            </div>

            <div class="text-left" style="margin-left:20mm; margin-right:20mm;">

                <t t-if="docs.date_start">
                    <div>
                        <label style="width:240px;">
                            Date
                        </label>
                        :&amp;nbsp;
                        <t t-esc="docs.date_start.strftime('%A, %B %d, %Y')"/>
                    </div>
                </t>
                <t t-if="docs.name">
                    <div>
                        <label style="width:240px;">
                            <t t-if="docs.type =='sale'">
                                S.O. number
                            </t>
                            <t t-else="">
                                P.O. number
                            </t>
                        </label>
                        :&amp;nbsp;
                        <t t-if="docs.type =='sale'">
                            <t t-esc="docs.sale_report_name"/>
                        </t>
                        <t t-else="">
                            <t t-esc="docs.purchase_report_name"/>
                        </t>
                    </div>
                </t>
                <div style="font-weight: bold; padding-top:18px;">
                    Specification
                </div>
                <div style="padding-top:8px;">
                    <label style="width:240px;">
                        Product
                    </label>
                    <span>
                        :&amp;nbsp;
                        <t t-esc="result.product_id.name"/>
                    </span>
                    <t t-if="result.name">
                        <div style="padding-left:255px;">
                            <t t-esc="result.name"/>
                        </div>
                    </t>
                </div>
                <t t-if="docs.ffa">
                    <div>
                        <label style="width:240px; padding-top:6px;">
                            FFA
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            &amp;nbsp;
                            <t t-esc="docs.ffa"/>
                        </label>
                        <t t-esc="docs.ffa_min_max"/>
                    </div>
                </t>
                <t t-if="docs.m_and_i">
                    <div>
                        <label style="width:240px;">
                            M&amp;I
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            &amp;nbsp;
                            <t t-esc="docs.m_and_i"/>
                        </label>
                        <t t-esc="docs.m_and_i_min_max"/>
                    </div>
                </t>
                <t t-if="docs.iv">
                    <div>
                        <label style="width:240px;">
                            IV
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            &amp;nbsp;
                            <t t-esc="docs.iv"/>
                        </label>
                        <t t-esc="docs.iv_min_max"/>
                    </div>
                </t>
                <t t-if="docs.sulphur">
                    <div>
                        <label style="width:240px;">
                            Sulphur
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            &amp;nbsp;
                            <t t-esc="docs.sulphur"/>
                        </label>
                        <t t-esc="docs.sulphur_iv_min_max"/>
                    </div>
                </t>
                <t t-if="docs.country_id">
                    <div>
                        <label style="width:240px;">
                            Country of origin
                        </label>
                        :&amp;nbsp;
                        <t t-esc="docs.country_id.name"/>
                    </div>
                </t>
                <t t-if="docs.appearance">
                    <div class="row mx-0">
                        <div>
                            <label style="width:240px;">
                                Appearance
                            </label>
                            :&amp;nbsp;
                        </div>
                        <div class="col-8 px-0 pl-1">
                            <t t-esc="docs.appearance"/>

                        </div>
                    </div>
                </t>
                <div>
                    <label style="width:240px;margin-bottom: 0rem;">
                        Prohibited materials
                    </label>
                    :&amp;nbsp;
                    Distillation's residues, acids oil, detergents, waxes, mineral oil,
                    <br/>
                    <label style="padding-left:255px; margin-bottom: 0rem;">
                        esters, industrial chemicals, animal fat or any material of unknown origin (which in itself is
                        unsuitable to produce
                        biofuel)
                    </label>
                </div>
                <div style="font-weight: bold; padding-top:18px;">
                    Commercial terms
                </div>
                <!--                                    <t t-if="form['amount']">-->
                <div style="padding-top:10px;">
                    <label style="width:240px;">
                        Price per MT
                    </label>
                    :&amp;nbsp;
                    <t t-esc="result.price_unit"/>
                    &amp;nbsp;
                    <t t-esc="docs.currency_id.name"/>
                </div>
                <div>
                    <label style="width:240px;">
                        Quantity
                    </label>
                    :&amp;nbsp;
                    <t t-esc="result.quantity"/>&amp;nbsp;MT +/- 5% weight difference allowed
                </div>
                <t t-if="docs.packaging">
                    <div class="row mx-0">
                        <div>
                            <label style="width:240px;">
                                Packaging
                            </label>
                            :&amp;nbsp;
                        </div>
                        <div class="col-8 px-0 pl-1">
                            <t t-esc="docs.packaging"/>
                        </div>
                    </div>
                </t>
                <t t-if="docs.expected_containers">
                    <div class="row mx-0">
                        <div>
                            <label style="width:240px;">
                                Expected amount of containers
                            </label>
                            :&amp;nbsp;
                        </div>
                        <div class="col-8 px-0 pl-1">
                            <t t-esc="docs.expected_containers"/>

                        </div>
                    </div>
                </t>
                <t t-if="docs.delivery_term_id">
                    <div class="row mx-0">
                        <div>
                            <label style="width:240px;">
                                Delivery terms
                            </label>
                            :&amp;nbsp;
                        </div>
                        <div class="col-8 px-0 pl-1">
                            <t t-esc="docs.delivery_term_id.name"/>
                        </div>
                    </div>
                </t>
                <t t-if="docs.period_f_delivery">
                    <div>
                        <label style="width:240px;">
                            Period of shipment and delivery
                        </label>
                        :&amp;nbsp;
                        <t t-esc="docs.period_f_delivery.strftime('%B,%Y')"/>
                        <t t-if="docs.period_f_delivery_end">
                            &amp;nbsp; to &amp;nbsp;
                            <t t-esc="docs.period_f_delivery_end.strftime('%B,%Y')"/>
                        </t>
                    </div>
                </t>
                <t t-if="docs.payment_term_id">
                    <div>
                        <label style="width:240px;">
                            Payment terms
                        </label>
                        :&amp;nbsp;
                        <t t-esc="docs.payment_term_id.name"/>
                    </div>
                </t>
                <t t-if="docs.trading_rule_id">
                    <div>
                        <label style="width:240px;">
                            Applied trading rules
                        </label>
                        :&amp;nbsp;
                        <t t-esc="docs.trading_rule_id.name"/>
                    </div>
                </t>
                <t t-if="docs.legislation_id">
                    <div>
                        <label style="width:240px;">
                            Legislation
                        </label>
                        :&amp;nbsp;
                        <t t-esc="docs.legislation_id.name"/>
                    </div>
                </t>
                <div style="padding-top:10px;">
                    In deviation from the NOFOTA trading rules, when the buyer can show irrevocable
                    proof of
                    payment and the payment is not
                    received by seller due to international OFAC investigation of the seller, this
                    purchase
                    order is still lawfully binding.
                </div>
                <div style="font-weight: bold; padding-top:18px;">
                    Discount clause
                </div>
                <div style="padding-top:8px;">
                    In the even the agreed upon values are exceeded a discount is applicable
                    according to
                    below numbers and percentages.
                </div>
                <t t-if="docs.ffa">
                    <div style="padding-top:3px;">
                        <label style="width:240px;">
                            FFA
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            <t t-if="docs.ffa_min_max=='min'">
                                Below&amp;nbsp;
                                <t t-esc="docs.ffa"/>
                            </t>
                            <t t-else="">
                                Above&amp;nbsp;
                                <t t-esc="docs.ffa"/>
                            </t>
                        </label>
                        1% discount per 1 % off spec
                    </div>
                </t>
                <t t-if="docs.m_and_i">
                    <div>
                        <label style="width:240px;">
                            M&amp;I
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            <t t-if="docs.m_and_i_min_max=='min'">
                                Below&amp;nbsp;
                                <t t-esc="docs.m_and_i"/>
                            </t>
                            <t t-else="">
                                Above&amp;nbsp;
                                <t t-esc="docs.m_and_i"/>
                            </t>
                        </label>
                        1% discount per 1 % off spec
                    </div>
                </t>
                <t t-if="docs.iv">
                    <div>
                        <label style="width:240px;">
                            Iodine Value
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            <t t-if="docs.iv_min_max=='min'">
                                Below&amp;nbsp;
                                <t t-esc="docs.iv"/>
                            </t>
                            <t t-else="">
                                Above&amp;nbsp;
                                <t t-esc="docs.iv"/>
                            </t>
                        </label>
                        1 USD discount per point off spec
                    </div>
                </t>
                <t t-if="docs.sulphur">
                    <div>
                        <label style="width:240px;">
                            Sulphur
                        </label>
                        :&amp;nbsp;
                        <label style="width:135px;">
                            <t t-if="docs.sulphur_iv_min_max=='min'">
                                Below&amp;nbsp;
                                <t t-esc="docs.sulphur"/>
                            </t>
                            <t t-else="">
                                Above&amp;nbsp;
                                <t t-esc="docs.sulphur"/>
                            </t>
                        </label>
                        1 USD discount per point off spec
                    </div>
                </t>
            </div>
        </template>

